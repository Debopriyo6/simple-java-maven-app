pipeline {
    agent any
    stages{
        
   
        stage('Test') {
            steps {
                sh 'mvn test'
            }
            post {
                always {
                    junit 'target/surefire-reports/*.xml'
                }
            }
        }
        stage('Deliver') {
            steps {
                echo "Delivered"
            }
        }
        stage('MultiBranch'){
            steps{
                echo "Multi Branch Set-Up"
            }
        }
    }
    }

